        <span class="form__review-name">{{name}}</span>
        <span class="form__review-place">{{place}}</span>
        {{!-- <span class="form__review-date">{{id}}</span> --}}
    </div>
    <div class="form__review-row">
        <span class="form__review-comment">{{message}}</span>
    </div>
</div>
{{/each}}
